    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link": "^1.2.14",
    "apollo-link-context": "^1.0.20",
    "apollo-link-http": "^1.5.17",
    "apollo-link-ws": "^1.0.20",
    "apollo-utilities": "^1.3.4",
    "graph-app-kit": "^1.0.4",
    "graphql": "^15.1.0",
    "html2canvas": "^1.0.0-rc.1",
    "neo4j-driver": "^4.0.2",
    "react": "^16.7.0",
